{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-crafto/md-number-text",
  "version": "0.1.0",
  "title": "MD Number Text",
  "apiVersion": 3,
  "category": "md-crafto",
  "icon": "images-alt",
  "description": "A block to display a number with a text.",
  "keywords": [
    "md-crafto",
    "md-number-text"
  ],
  "supports": {
    "html": false
  },
  "textdomain": "md-crafto",
  "attributes": {
    "columnList": {
      "type": "array",
      "default": [
        {
          "id": 1,
          "title": "",
          "description": ""
        }
      ]
    },
    "columnTitleFontColor": {
      "type": "string",
      "default": ""
    },
    "columnDescriptionFontColor": {
      "type": "string",
      "default": ""
    },
    "borderColor": {
      "type": "string",
      "default": ""
    },
    "backgroundColor": {
      "type": "string",
      "default": ""
    },
    "columns": {
      "type": "number",
      "default": 3
    },
    "enableBorder": {
      "type": "boolean",
      "default": false
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}