{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-crafto-design/md-image-banner-faq",
  "version": "0.1.0",
  "title": "MD Image Banner FAQ",
  "apiVersion": 3,
  "category": "md-crafto-design",
  "icon": "format-image",
  "description": "A block that displays a banner with an image, text and faqs.",
  "keywords": ["banner", "image", "faq"],
  "supports": {
    "html": false,
    "align": ["wide", "full"],
    "anchor": true
  },
  "textdomain": "md-crafto-design",
  "attributes": {
    "subTitle": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "headingContent": {
      "type": "string",
      "default": ""
    },
    "buttonLink": {
      "type": "string",
      "default": ""
    },
    "beautyTipsText": {
      "type": "string",
      "default": ""
    },
    "mediaPosition": {
      "type": "string",
      "default": ""
    },
    "backgroundColor": {
      "type": "string",
      "default": ""
    },
    "subTitleColor": {
      "type": "string",
      "default": ""
    },
    "titleColor": {
      "type": "string",
      "default": ""
    },
    "headingContentColor": {
      "type": "string",
      "default": ""
    },
    "showSubTitle": {
      "type": "boolean",
      "default": true
    },
    "showTitle": {
      "type": "boolean",
      "default": true
    },
    "showHeadingContent": {
      "type": "boolean",
      "default": true
    },
    "showButton": {
      "type": "boolean",
      "default": true
    },
    "showBeautyTips": {
      "type": "boolean",
      "default": true
    },
    "mediaImage": {
      "type": "string",
      "default": ""
    },
    "backgroundImage": {
      "type": "string",
      "default": ""
    },
    "buttonColor": {
			"type": "string",
			"default": ""
		},
		"buttonBackgroundColor": {
			"type": "string",
			"default": ""
		},
		"buttonHoverColor": {
			"type": "string",
			"default": ""
		},
		"buttonHoverBackgroundColor": {
			"type": "string",
			"default": ""
		},
    "beautyTipsTextColor": {
      "type": "string",
      "default": ""
    },
    "showFaq": {
      "type": "boolean",
      "default": true
    },
    "faqItems": {
      "type": "array",
      "default": []
    },
    "faqItemTitleColor": {
      "type": "string",
      "default": ""
    },
    "faqItemContentColor": {
      "type": "string",
      "default": ""
    },
    "beautyTipsItems": {
      "type": "array",
      "default": []
    },
    "beautyTipsItemColor": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}