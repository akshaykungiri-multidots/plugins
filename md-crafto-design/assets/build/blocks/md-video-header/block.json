{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-crafto-beauty/md-video-header",
  "version": "0.1.0",
  "title": "MD Video Header",
  "apiVersion": 3,
  "category": "md-crafto-beauty",
  "icon": "video-alt2",
  "description": "A block that displays a video header with a heading and subheading.",
  "keywords": [
    "md-crafto-beauty",
    "md-video-header"
  ],
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "textdomain": "md-crafto-beauty",
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "subTitle": {
      "type": "string",
      "default": ""
    },
    "backgroundImage": {
      "type": "string",
      "default": ""
    },
    "enableOverlay": {
      "type": "boolean",
      "default": true
    },
    "overlayColor": {
      "type": "string",
      "default": "#000000"
    },
    "overlayOpacity": {
      "type": "number",
      "default": 0.5
    },
    "backgroundColor": {
      "type": "string",
      "default": ""
    },
    "titleColor": {
      "type": "string",
      "default": ""
    },
    "subTitleColor": {
      "type": "string",
      "default": ""
    },
    "showTitle": {
      "type": "boolean",
      "default": true
    },
    "showSubTitle": {
      "type": "boolean",
      "default": true
    },
    "mediaurl": {
      "type": "string",
      "default": ""
    },
    "youtubeurl": {
      "type": "string",
      "default": ""
    },
    "videotype": {
      "type": "string",
      "default": "media-upload"
    }
  },
  "editorScript": "file:./index.js",
  "viewScript": [
    "file:./view.js"
  ],
  "style": "file:./style-index.css"
}