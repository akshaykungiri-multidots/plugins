{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-crafto-beauty/md-map",
  "version": "0.1.0",
  "title": "MD Map",
  "apiVersion": 3,
  "category": "md-crafto-beauty",
  "icon": "star-filled",
  "description": "A block to display a logo with text.",
  "keywords": [
    "md-crafto-beauty",
    "md-map"
  ],
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "textdomain": "md-crafto-beauty",
  "attributes": {
    "heading": {
      "type": "string",
      "default": ""
    },
    "showHeading": {
      "type": "boolean",
      "default": true
    },
    "headingColor": {
      "type": "string",
      "default": ""
    },
    "shopName": {
      "type": "string",
      "default": ""
    },
    "shopAddress": {
      "type": "string",
      "default": ""
    },
    "shopPhone": {
      "type": "string",
      "default": ""
    },
    "shopEmail": {
      "type": "string",
      "default": ""
    },
    "showShopName": {
      "type": "boolean",
      "default": true
    },
    "showShopAddress": {
      "type": "boolean",
      "default": true
    },
    "showShopPhone": {
      "type": "boolean",
      "default": true
    },
    "showShopEmail": {
      "type": "boolean",
      "default": true
    },
    "shopNameColor": {
      "type": "string",
      "default": ""
    },
    "shopAddressColor": {
      "type": "string",
      "default": ""
    },
    "shopPhoneColor": {
      "type": "string",
      "default": ""
    },
    "shopEmailColor": {
      "type": "string",
      "default": ""
    },
    "googleMapLinkText": {
      "type": "string",
      "default": ""
    },
    "showGoogleMapLink": {
      "type": "boolean",
      "default": true
    },
    "googleMapLinkTextColor": {
      "type": "string",
      "default": ""
    },
    "boxBackgroundColor": {
      "type": "string",
      "default": ""
    },
    "mapIframeUrl": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}