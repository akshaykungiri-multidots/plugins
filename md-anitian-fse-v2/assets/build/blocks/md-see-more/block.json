{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-anitian-fse-v2/md-see-more",
  "version": "0.1.0",
  "title": "MD See More",
  "apiVersion": 3,
  "category": "md-anitian-fse-v2",
  "icon": "arrow-right-alt",
  "description": "A block to display the see more",
  "keywords": [
    "md-anitian-fse-v2",
    "md-see-more"
  ],
  "supports": {
    "html": false
  },
  "textdomain": "md-anitian-fse-v2",
  "attributes": {
    "heading": {
      "type": "string",
      "default": "This is Default Heading"
    },
    "headingColor": {
      "type": "string",
      "default": "#000"
    },
    "content": {
      "type": "string",
      "default": "This is Default Content....."
    },
    "contentColor": {
      "type": "string",
      "default": "#000"
    },
    "buttonText": {
      "type": "string",
      "default": "CLICK HERE TO SEE MORE"
    },
    "buttonColor": {
      "type": "string",
      "default": "#fff"
    },
    "buttonBGColor": {
      "type": "string",
      "default": "#f75227"
    },
    "showContent": {
      "type": "boolean",
      "default": true
    }
  },
  "editorScript": "file:./index.js",
  "viewScript": [
    "file:./view.js"
  ],
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}