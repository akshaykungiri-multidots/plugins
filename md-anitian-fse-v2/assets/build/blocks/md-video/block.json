{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-anitian-fse-v2/md-video",
  "version": "0.1.0",
  "title": "MD Video",
  "apiVersion": 3,
  "category": "md-anitian-fse-v2",
  "icon": "format-video",
  "description": "A block to display the video",
  "keywords": [
    "md-anitian-fse-v2",
    "md-video"
  ],
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "textdomain": "md-anitian-fse-v2",
  "attributes": {
    "mediaurl": {
      "type": "string",
      "default": ""
    },
    "youtubeurl": {
      "type": "string",
      "default": ""
    },
    "videotype": {
      "type": "string",
      "default": "media-upload"
    },
    "showPlayButton": {
      "type": "boolean",
      "default": true
    },
    "playButtonImage": {
      "type": "string",
      "default": ""
    },
    "showPlayIconTitle": {
      "type": "boolean",
      "default": true
    },
    "playIconTitle": {
      "type": "string",
      "default": "Play Icon"
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}