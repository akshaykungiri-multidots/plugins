{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-pofo/md-video-header",
  "version": "0.1.0",
  "title": "MD Video Header",
  "apiVersion": 3,
  "category": "md-pofo",
  "icon": "video-alt2",
  "description": "A block that displays a video header with a heading and subheading.",
  "keywords": [
    "md-pofo",
    "md-video-header"
  ],
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "textdomain": "md-pofo",
  "attributes": {
    "blockId": {
      "type": "string",
      "default": ""
    },
    "heading": {
      "type": "string",
      "default": ""
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "buttonText": {
      "type": "string",
      "default": ""
    },
    "backgroundImage": {
      "type": "string",
      "default": ""
    },
    "enableOverlay": {
      "type": "boolean",
      "default": false
    },
    "overlayColor": {
      "type": "string",
      "default": ""
    },
    "overlayOpacity": {
      "type": "number",
      "default": 0.75
    },
    "backgroundColor": {
      "type": "string",
      "default": ""
    },
    "headingColor": {
      "type": "string",
      "default": ""
    },
    "showHeading": {
      "type": "boolean",
      "default": true
    },
    "showDescription": {
      "type": "boolean",
      "default": true
    },
    "showButton": {
      "type": "boolean",
      "default": true
    },
    "descriptionColor": {
      "type": "string",
      "default": ""
    },
    "buttonColor": {
      "type": "string",
      "default": ""
    },
    "buttonBackgroundColor": {
      "type": "string",
      "default": ""
    },
    "buttonHoverColor": {
      "type": "string",
      "default": ""
    },
    "buttonHoverBackgroundColor": {
      "type": "string",
      "default": ""
    },
    "mediaurl": {
      "type": "string",
      "default": ""
    },
    "youtubeurl": {
      "type": "string",
      "default": ""
    },
    "videotype": {
      "type": "string",
      "default": "media-upload"
    },
    "videoFooterText": {
      "type": "string",
      "default": ""
    },
    "showVideoFooterText": {
      "type": "boolean",
      "default": true
    },
    "videoFooterTextColor": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:./index.js",
  "viewScript": [
    "file:./view.js"
  ],
  "style": "file:./style-index.css"
}