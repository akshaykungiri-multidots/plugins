{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-healthstream/md-resource-slider",
  "version": "0.1.0",
  "title": "MD Resource Slider",
  "apiVersion": 3,
  "category": "md-healthstream",
  "icon": "format-gallery",
  "description": "A block to display resources in a slider",
  "keywords": [
    "md-healthstream",
    "md-resource-slider"
  ],
  "supports": {
    "html": false
  },
  "textdomain": "md-healthstream",
  "attributes": {
    "heading": {
      "type": "string",
      "default": "This is Default Heading"
    },
    "autoplay": {
      "type": "boolean",
      "default": false
    },
    "arrows": {
      "type": "boolean",
      "default": true
    },
    "dots": {
      "type": "boolean",
      "default": false
    },
    "slideInfinite": {
      "type": "boolean",
      "default": false
    },
    "slideSlidesToShow": {
      "type": "number",
      "default": 1
    },
    "slideSlidesToScroll": {
      "type": "number",
      "default": 1
    },
    "showHeading": {
      "type": "boolean",
      "default": true
    },
    "showTitle": {
      "type": "boolean",
      "default": true
    },
    "showExcerpt": {
      "type": "boolean",
      "default": true
    },
    "showFeaturedImage": {
      "type": "boolean",
      "default": true
    },
    "headingColor": {
      "type": "string",
      "default": ""
    },
    "titleColor": {
      "type": "string",
      "default": ""
    },
    "excerptColor": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:./index.js",
  "viewScript": [
    "file:./view.js"
  ],
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}