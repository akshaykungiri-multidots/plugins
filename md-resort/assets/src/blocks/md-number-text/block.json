{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-resort/md-number-text",
  "version": "0.1.0",
  "title": "MD Number Text",
  "apiVersion": 3,
  "category": "md-resort",
  "icon": "editor-ol",
  "description": "A block to display a number with a text.",
  "keywords": ["md-resort", "md-number-text"],
  "supports": {
    "html": false
  },
  "textdomain": "md-resort",
  "attributes": {
    "columnList": {
      "type": "array",
      "default": [
        {
          "id": 1,
          "title": "",
          "description": ""
        }
      ]
    },
    "columnTitleFontColor": {
      "type": "string",
      "default": ""
    },
    "columnDescriptionFontColor": {
      "type": "string",
      "default": ""
    },
    "borderColor": {
      "type": "string",
      "default": ""
    },
    "backgroundColor": {
      "type": "string",
      "default": ""
    },
    "columns": {
			"type": "number",
			"default": 3
		},
    "enableBorder": {
      "type": "boolean",
      "default": false
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}