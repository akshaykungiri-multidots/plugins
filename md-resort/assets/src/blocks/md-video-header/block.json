{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-resort/md-video-header",
  "version": "0.1.0",
  "title": "MD Video Header",
  "apiVersion": 3,
  "category": "md-resort",
  "icon": "video-alt2",
  "description": "A block that displays a video header with a heading and subheading.",
  "keywords": ["md-resort", "md-video-header"],
  "supports": {
    "html": false,
    "align": ["wide", "full"]
  },
  "textdomain": "md-resort",
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "typedText": {
      "type": "array",
      "default": [
        {
          "id": 1,
          "text": ""
        }
      ]
    },
    "backgroundImage": {
      "type": "string",
      "default": ""
    },
    "backgroundColor": {
      "type": "string",
      "default": ""
    },
    "titleColor": {
      "type": "string",
      "default": ""
    },
    "showTitle": {
      "type": "boolean",
      "default": true
    },
    "mediaurl": {
      "type": "string",
      "default": ""
    },
    "youtubeurl": {
      "type": "string",
      "default": ""
    },
    "videotype": {
      "type": "string",
      "default": "media-upload"
    },
    "videoFooterText": {
      "type": "string",
      "default": ""
    },
    "showVideoFooterText": {
      "type": "boolean",
      "default": true
    },
    "videoFooterTextColor": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:./index.js",
  "viewScript": [ "file:./view.js" ],
  "style": "file:./style-index.css"
}