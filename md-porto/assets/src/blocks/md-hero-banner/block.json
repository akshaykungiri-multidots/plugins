{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-porto/md-hero-banner",
  "version": "0.1.0",
  "title": "MD Hero Banner",
  "apiVersion": 3,
  "category": "md-porto",
  "icon": "format-image",
  "description": "A block that displays a banner with an image and text.",
  "keywords": ["md-porto", "md-hero-banner"],
  "supports": {
    "html": false,
    "align": ["wide", "full"],
    "anchor": true
  },
  "textdomain": "md-porto",
  "attributes": {
    "subTitle": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "mediaPosition": {
      "type": "string",
      "default": ""
    },
    "backgroundColor": {
      "type": "string",
      "default": ""
    },
    "subTitleColor": {
      "type": "string",
      "default": ""
    },
    "subTitleBackgroundColor": {
      "type": "string",
      "default": ""
    },
    "titleColor": {
      "type": "string",
      "default": ""
    },
    "showSubTitle": {
      "type": "boolean",
      "default": true
    },
    "showTitle": {
      "type": "boolean",
      "default": true
    },
    "showMedia": {
      "type": "boolean",
      "default": true
    },
    "mediaImage": {
      "type": "string",
      "default": ""
    },
    "showBreadcrumb": {
      "type": "boolean",
      "default": true
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}