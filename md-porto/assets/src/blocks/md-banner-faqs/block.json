{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-porto/md-banner-faqs",
  "version": "0.1.0",
  "title": "MD Banner FAQs",
  "apiVersion": 3,
  "category": "md-porto",
  "icon": "admin-comments",
  "titleContent": "A block to display a banner with FAQs",
  "keywords": ["banner", "faqs"],
  "supports": {
    "html": false,
    "align": ["wide", "full"]
  },
  "textdomain": "md-porto",
  "attributes": {
    "subTitle": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "titleContent": {
      "type": "string",
      "default": ""
    },
    "buttonText": {
      "type": "string",
      "default": ""
    },
    "faqs": {
      "type": "array",
      "default": []
    },
    "showSubTitle": {
      "type": "boolean",
      "default": true
    },
    "showTitle": {
      "type": "boolean",
      "default": true
    },
    "showTitleContent": {
      "type": "boolean",
      "default": true
    },
    "showButton": {
      "type": "boolean",
      "default": true
    },
    "showFaqs": {
      "type": "boolean",
      "default": true
    },
    "subTitleColor": {
      "type": "string",
      "default": ""
    },
    "titleColor": {
      "type": "string",
      "default": ""
    },
    "titleContentColor": {
      "type": "string",
      "default": ""
    },
    "buttonColor": {
			"type": "string",
			"default": ""
		},
		"buttonBackgroundColor": {
			"type": "string",
			"default": ""
		},
		"buttonHoverColor": {
			"type": "string",
			"default": ""
		},
		"buttonHoverBackgroundColor": {
			"type": "string",
			"default": ""
		},
    "faqsTextColor": {
      "type": "string",
      "default": ""
    },
    "faqsListBackgroundColor": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}