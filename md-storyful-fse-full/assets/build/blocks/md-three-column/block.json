{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-storyful-fse-full/md-three-column",
  "version": "0.1.0",
  "title": "MD Three column Block",
  "apiVersion": 3,
  "category": "md-storyful-fse-full",
  "icon": "columns",
  "description": "A Three column block with image, title, description and button",
  "keywords": [
    "md-three-column",
    "Block"
  ],
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "textdomain": "md-storyful-fse-full",
  "attributes": {
    "heading": {
      "type": "string",
      "default": ""
    },
    "backgroundImage": {
      "type": "string",
      "default": ""
    },
    "columnList": {
      "type": "array",
      "default": [
        {
          "id": 1,
          "title": "",
          "description": "",
          "image": ""
        }
      ]
    },
    "buttonText": {
      "type": "string",
      "default": ""
    },
    "headingFontColor": {
      "type": "string",
      "default": ""
    },
    "columnTitleFontColor": {
      "type": "string",
      "default": ""
    },
    "columnDescriptionFontColor": {
      "type": "string",
      "default": ""
    },
    "showHeading": {
      "type": "boolean",
      "default": true
    },
    "showColumnTitle": {
      "type": "boolean",
      "default": true
    },
    "showColumnDescription": {
      "type": "boolean",
      "default": true
    },
    "showColumnImage": {
      "type": "boolean",
      "default": true
    },
    "showButton": {
      "type": "boolean",
      "default": true
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}