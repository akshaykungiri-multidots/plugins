{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-storyful-fse-full/md-counter",
  "version": "0.1.0",
  "title": "MD Counter Block",
  "apiVersion": 3,
  "category": "md-storyful-fse-full",
  "icon": "dashboard",
  "description": "A  block designed to display numerical values or statistics, often used to highlight achievements or key metrics.",
  "keywords": [
    "md-counter",
    "Block"
  ],
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "textdomain": "md-storyful-fse-full",
  "attributes": {
    "columnList": {
      "type": "array",
      "default": [
        {
          "id": 1,
          "title": "",
          "description": ""
        }
      ]
    },
    "columnTitleFontSize": {
      "type": "string",
      "default": "16"
    },
    "columnDescriptionFontSize": {
      "type": "number",
      "default": "16"
    },
    "columnTitleFontColor": {
      "type": "string",
      "default": ""
    },
    "columnDescriptionFontColor": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}