{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-pointcentral-fse/md-hero-banner",
  "version": "0.1.0",
  "title": "MD Hero Banner",
  "apiVersion": 3,
  "category": "md-pointcentral-fse",
  "icon": "cover-image",
  "description": "A block to display a hero banner",
  "keywords": ["md-pointcentral-fse", "hero", "banner"],
  "supports": {
    "html": false,
    "align": ["wide", "full"]
  },
  "textdomain": "md-pointcentral-fse",
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "buttonText": {
      "type": "string",
      "default": ""
    },
    "bannerImage": {
      "type": "string",
      "default": ""
    },
    "backgroundColor": {
      "type": "string",
      "default": ""
    },
    "titleColor": {
      "type": "string",
      "default": ""
    },
    "descriptionColor": {
      "type": "string",
      "default": ""
    },
    "buttonTextColor": {
      "type": "string",
      "default": ""
    },
    "buttonBackgroundColor": {
      "type": "string",
      "default": ""
    },
    "showTitle": {
      "type": "boolean",
      "default": true
    },
    "showDescription": {
      "type": "boolean",
      "default": true
    },
    "showButton": {
      "type": "boolean",
      "default": true
    },
    "showImage": {
      "type": "boolean",
      "default": true
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}