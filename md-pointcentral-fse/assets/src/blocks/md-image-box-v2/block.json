{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-pointcentral-fse/md-image-box-v2",
  "version": "0.1.0",
  "title": "MD Image Box v2",
  "apiVersion": 3,
  "category": "md-pointcentral-fse",
  "icon": "images-alt2",
  "description": "A block that displays an image with a title and description.",
  "keywords": ["md-pointcentral-fse", "md-image-box-v2"],
  "supports": {
    "html": false,
    "align": ["wide", "full"]
  },
  "textdomain": "md-pointcentral-fse",
  "attributes": {
    "subHeading": {
      "type": "string",
      "default": ""
    },
    "heading": {
      "type": "string",
      "default": ""
    },
    "imageBoxes": {
      "type": "array",
      "default": [
      ]
    },
    "enableSlider": {
      "type": "boolean",
      "default": false
    },
    "subHeadingColor": {
      "type": "string",
      "default": ""
    },
    "headingColor": {
      "type": "string",
      "default": ""
    },
    "imageBoxBackgroundColor": {
      "type": "string",
      "default": ""
    },
    "imageBoxTitleColor": {
      "type": "string",
      "default": ""
    },
    "imageBoxContentColor": {
      "type": "string",
      "default": ""
    },
    "imageBoxButtonColor": {
      "type": "string",
      "default": ""
    },
    "showSubHeading": {
      "type": "boolean",
      "default": true
    },
    "showHeading": {
      "type": "boolean",
      "default": true
    },
    "showImage": {
      "type": "boolean",
      "default": true
    },
    "showImageBoxContent": {
      "type": "boolean",
      "default": true
    },
    "showImageBoxButton": {
      "type": "boolean",
      "default": true
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}