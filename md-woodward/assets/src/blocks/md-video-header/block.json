{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-woodward/md-video-header",
  "version": "0.1.0",
  "title": "MD Video Header",
  "apiVersion": 3,
  "category": "md-woodward",
  "icon": "video-alt2",
  "description": "A block that displays a video header with a heading and subheading.",
  "keywords": ["md-woodward", "md-video-header"],
  "supports": {
    "html": false,
    "align": ["wide", "full"]
  },
  "textdomain": "md-woodward",
  "attributes": {
    "subTitle": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "headingContent": {
      "type": "string",
      "default": ""
    },
    "buttonLink": {
      "type": "string",
      "default": ""
    },
    "videoPosterImage": {
      "type": "string",
      "default": ""
    },
    "backgroundColor": {
      "type": "string",
      "default": ""
    },
    "subTitleColor": {
      "type": "string",
      "default": ""
    },
    "titleColor": {
      "type": "string",
      "default": ""
    },
    "headingContentColor": {
      "type": "string",
      "default": ""
    },
    "showSubTitle": {
      "type": "boolean",
      "default": true
    },
    "showTitle": {
      "type": "boolean",
      "default": true
    },
    "showHeadingContent": {
      "type": "boolean",
      "default": true
    },
    "showButton": {
      "type": "boolean",
      "default": true
    },
    "mediaurl": {
      "type": "string",
      "default": ""
    },
    "youtubeurl": {
      "type": "string",
      "default": ""
    },
    "videotype": {
      "type": "string",
      "default": "media-upload"
    },
    "buttonColor": {
			"type": "string",
			"default": ""
		},
		"buttonBackgroundColor": {
			"type": "string",
			"default": ""
		},
		"buttonHoverColor": {
			"type": "string",
			"default": ""
		},
		"buttonHoverBackgroundColor": {
			"type": "string",
			"default": ""
		}
  },
  "editorScript": "file:./index.js",
  "viewScript": [ "file:./view.js" ],
  "style": "file:./style-index.css"
}