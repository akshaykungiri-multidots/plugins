{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "md-putco/md-counter",
  "version": "0.1.0",
  "title": "MD Counter",
  "apiVersion": 3,
  "category": "md-putco",
  "icon": "chart-bar",
  "description": "A block to display counter with animation",
  "keywords": ["md-counter", "Block"],
  "supports": {
    "html": false
  },
  "textdomain": "md-putco",
  "attributes": {
    "counterNumber": {
      "type": "string",
      "default": "0"
    },
    "counterSign": {
      "type": "string",
      "default": ""
    },
    "alignText": {
      "type": "string",
      "default": "center"
    },
    "isSignBefore": {
      "type": "boolean",
      "default": false
    },
    "counterText": {
      "type": "string",
      "default": ""
    },
    "counterNumberColor": {
      "type": "string",
      "default": "#000000"
    },
    "counterTextColor": {
      "type": "string",
      "default": "#000000"
    },
    "showCounterSign": {
      "type": "boolean",
      "default": true
    },
    "showCounterText": {
      "type": "boolean",
      "default": true
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "viewScript": [ "file:./view.js" ]
}